{
  "manifest_version": 3,
  "name": "Easy Snap",
  "description": "Create a simple mockup of the current tab with different viewports in a click!",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "mobile_popup_icon.png"
    }
  },
  "permissions": ["activeTab"]
}
